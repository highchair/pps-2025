          "name": "Pencil Yellow Tint",
          "slug": "secondary-tint"
        },
        {
          "color": "#EABA6B",
          "name": "Pencil Yellow",
          "slug": "secondary"
        }
      ],
      "duotone": [
        {
          "colors": [
            "#8b4513",
            "#f0e68c"
          ],
          "name": "Sepia",
          "slug": "sepia"
        }
      ],
      "gradients": [
        {
          "gradient": "linear-gradient(45deg,rgba(from var(--wp--preset--color--primary-tint) r g b / 0.5) 5%, rgba(from var(--wp--preset--color--primary-tint) r g b / 1) 95%)",
          "name": "Light Jewel",
          "slug": "grad-primary"
        }
      ]
    },
    "dimensions": {
      "minHeight": false
    },
    "layout": {
      "contentSize": "48rem",
      "wideSize": "75rem"
    },
    "position": {
      "sticky": false
    },
    "shadow": {
      "defaultPresets": false
    },
    "spacing": {
      "blockGap": true,
      "margin": true,
      "padding": true,
      "customSpacingSize": false,
      "spacingSizes": [
        {
          "name": "Extra Small",
          "size": "clamp(0.5rem, 0.35rem + 0.5vw, 0.75rem)",
          "slug": "x-sm"
        },
        {
          "name": "Small",
          "size": "clamp(1rem, 0.7rem + 1vw, 1.5rem)",
          "slug": "sm"
        },
        {
          "name": "Small to Medium",
          "size": "clamp(1rem, 0.4rem + 2vw, 2rem)",
          "slug": "sm-md"
        },
        {
          "name": "Medium",
          "size": "clamp(1.5rem, 1.2rem + 1vw, 2rem)",
          "slug": "md"
        },
        {
          "name": "Medium to XLarge",
          "size": "clamp(1.5rem, 0rem + 5vw, 4rem)",
          "slug": "md-xl"
        },
        {
          "name": "Large",
          "size": "clamp(2rem, 1.4rem + 2vw, 3rem)",
          "slug": "lg"
        },
        {
          "name": "XLarge",
          "size": "clamp(3rem, 2.4rem + 2vw, 4rem)",
          "slug": "xl"
        },
        {
          "name": "XL to Huge",
          "size": "clamp(3rem, 1.8rem + 4vw, 5rem)",
          "slug": "xl-xxl"
        },
        {
          "name": "Huge",
          "size": "clamp(4rem, 3.4rem + 2vw, 5rem)",
          "slug": "xxl"
        }
      ]
    },
    "typography": {
      "customFontSize": false,
      "fontStyle": true,
      "fontWeight": false,
      "letterSpacing": false,
      "lineHeight": false,
      "textColumns": true,
      "textDecoration": false,
      "textTransform": true,
      "writingMode": false,
      "fontFamilies": [
        {
          "name": "Default (Sans)",
          "slug": "default",
          "fontFamily": "'poppins', sans-serif"
        },
        {
          "name": "Accent (Serif)",
          "slug": "accent",
          "fontFamily": "'loretta-variable', serif"
        }
      ],
      "fluid": true,
      "fontSizes": [
        {
          "name": "Small",
          "size": "0.875rem",
          "slug": "sm",
          "fluid": {
            "min": "0.875rem",
            "max": "0.9375rem"
          }
        },
        {
          "name": "Normal",
          "size": "1rem",
          "slug": "base",
          "fluid": {
            "min": "1rem",
            "max": "1.125rem"
          }
        },
        {
          "name": "Header 6",
          "size": "1.2656rem",
