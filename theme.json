{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "border": {
      "color": true,
      "radius": false,
      "style": true,
      "width": true
    },
    "color": {
      "background": true,
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "link": false,
      "text": true,
      "palette": [
        {
          "color": "var(--c-white)",
          "name": "Default BG",
          "slug": "defaultBG"
        },
        {
          "color": "var(--c-rich-black)",
          "name": "Default BG",
          "slug": "defaultFG"
        },
        {
          "color": "var(--c-dark-jewel)",
          "name": "Dark Jewel",
          "slug": "d-primary"
        },
        {
          "color": "var(--c-blue-jewel)",
          "name": "Jewel",
          "slug": "primary"
        },
        {
          "color": "var(--c-blue-jewel-tint)",
          "name": "Blue Jewel Tint",
          "slug": "primary-tint"
        },
        {
          "color": "var(--c-blue-jewel-tint-light)",
          "name": "Blue Jewel Light Tint",
          "slug": "primary-l-tint"
        },
        {
          "color": "var(--c-yellow)",
          "name": "Pencil Yellow",
          "slug": "secondary"
        },
        {
          "color": "var(--c-yellow-tint)",
          "name": "Pencil Yellow Tint",
          "slug": "secondary-tint"
        },
        {
          "color": "var(--fc-border-default)",
          "name": "Border Gray",
          "slug": "border"
        },
        {
          "color": "var(--fc-border-light)",
          "name": "Border Gray Light",
          "slug": "border-l"
        }
      ],
      "duotone": [
        {
          "colors": [
            "#8b4513",
            "#f0e68c"
          ],
          "name": "Sepia",
          "slug": "sepia"
        }
      ],
      "gradients": [
        {
          "gradient": "var(--fc-grad-primary)",
          "name": "Light Jewel",
          "slug": "grad-primary"
        }
      ]
    },
    "dimensions": {
      "minHeight": false
    },
    "layout": {
      "contentSize": "40rem",
      "wideSize": "64rem"
    },
    "position": {
      "sticky": false
    },
    "shadow": {
      "defaultPresets": false
    },
    "spacing": {
      "blockGap": true,
      "margin": true,
      "padding": true,
      "customSpacingSize": false,
      "spacingSizes": [
        {
          "name": "Extra Small",
          "size": "var(--space-8-12)",
          "slug": "x-sm"
        },
        {
          "name": "Small",
          "size": "var(--space-16-24)",
          "slug": "sm"
        },
        {
          "name": "Small to Medium",
          "size": "var(--space-16-32)",
          "slug": "sm-md"
        },
        {
          "name": "Medium",
          "size": "var(--space-24-32)",
          "slug": "md"
        },
        {
          "name": "Medium to XLarge",
          "size": "var(--space-24-64)",
          "slug": "md-xl"
        },
        {
          "name": "Large",
          "size": "var(--space-32-48)",
          "slug": "lg"
        },
        {
          "name": "XLarge",
          "size": "var(--space-48-64)",
          "slug": "xl"
        },
        {
          "name": "XL to Huge",
          "size": "var(--space-48-80)",
          "slug": "xl-xxl"
        },
        {
          "name": "Huge",
          "size": "var(--space-64-80)",
          "slug": "xxl"
        }
      ]
    },
    "typography": {
      "customFontSize": false,
      "fontStyle": true,
      "fontWeight": false,
      "letterSpacing": false,
      "lineHeight": false,
      "textColumns": true,
      "textDecoration": false,
      "textTransform": true,
      "writingMode": false,
      "fontFamilies": [
        {
          "name": "Default (Sans)",
          "slug": "default",
          "fontFamily": "var(--ff-default)"
        },
        {
          "name": "Accent (Serif)",
          "slug": "accent",
          "fontFamily": "var(--ff-accent)"
        }
      ],
      "fluid": true,
      "fontSizes": [
        {
          "name": "Small",
          "size": "var(--fts-small)",
          "slug": "sm"
        },
        {
          "name": "Normal",
          "size": "var(--fts-base)",
          "slug": "base"
        },
        {
          "name": "Header 6",
          "size": "var(--fts-h6)",
          "slug": "h6"
        },
        {
          "name": "Header 5",
          "size": "var(--fts-h5)",
          "slug": "h5"
        },
        {
          "name": "Header 4",
          "size": "var(--fts-h4)",
          "slug": "h4"
        },
        {
          "name": "Header 3",
          "size": "var(--fts-h3)",
          "slug": "h3"
        },
        {
          "name": "Header 2",
          "size": "var(--fts-h2)",
          "slug": "h2"
        },
        {
          "name": "Header 1",
          "size": "var(--fts-h1)",
          "slug": "h1"
        },
        {
          "name": "Huge",
          "size": "var(--fts-xl)",
          "slug": "xl"
        },
        {
          "name": "Gigantic",
          "size": "var(--fts-xxl)",
          "slug": "xxl"
        }
      ]
    },
    "useRootPaddingAwareAlignments": true,
    "custom": {},
    "blocks": {}
  },
  "styles": {
    "blocks": {
      "core/group": {
        "color": {
          "text": "var(--fc-default-fg)",
          "background": "var(--fc-default-bg)",
          "link": "var(--fc-link-fg)"
        }
      },
      "core/paragraph": {
        "typography": {
          "fontFamily": "var(--ff-default)",
          "fontSize": "var(--fts-base, 100%)",
          "fontStyle": "normal",
          "fontWeight": "var(--fw-default-normal)",
          "letterSpacing": "0",
          "lineHeight": "var(--lh-normal)",
          "textDecoration": "none"
        }
      }
    },
    "border": {
      "color": "var(--b-color-default)",
      "style": "var(--b-style-default)",
      "width": "var(--b-size-default)"
    },
    "spacing": {
      "padding": {
        "top": "var(--space-container-block)",
        "bottom": "var(--space-container-block)",
        "left": "var(--space-container-inline)",
        "right": "var(--space-container-inline)"
      }
    }
  },
  "customTemplates": {},
  "templateParts": {},
  "patterns": []
}