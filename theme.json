          "slug": "gray-lt"
        },
        {
          "color": "#485b5b",
          "name": "Gray Medium",
          "slug": "gray-md"
        },
        {
          "color": "#242D2E",
          "name": "Rich Black",
          "slug": "default-fg"
        },
        {
          "color": "hsl(189, 82%, 96%)",
          "name": "Blue Jewel Light Tint",
          "slug": "primary-tint-lt"
        },
        {
          "color": "#D7F5FA",
          "name": "Blue Jewel Tint",
          "slug": "primary-tint"
        },
        {
          "color": "#016979",
          "name": "Jewel",
          "slug": "primary"
        },
        {
          "color": "#003239",
          "name": "Dark Jewel",
          "slug": "primary-dk"
        },
        {
